nel AS usc ON t.sessionid = usc.sessionid \n","  LEFT JOIN raw_data.session_transaction AS st ON usc.sessionid=st.sessionid\n","GROUP BY 1, 2;\n","\n","SELECT * FROM adhoc.ys_monthly_channel_summary"],"execution_count":null,"outputs":[{"output_type":"stream","text":[" * postgresql://guest:***@learnde.cduaw970ssvt.ap-northeast-2.redshift.amazonaws.com:5439/dev\n","Done.\n","Done.\n","42 rows affected.\n"],"name":"stdout"},{"output_type":"execute_result","data":{"text/html":["<table>\n","    <thead>\n","        <tr>\n","            <th>month</th>\n","            <th>channel</th>\n","            <th>uniqueusers</th>\n","            <th>paidusers</th>\n","            <th>conversionrate</th>\n","            <th>grossrevenue</th>\n","            <th>netrevenue</th>\n","        </tr>\n","    </thead>\n","    <tbody>\n","        <tr>\n","            <td>2019-05</td>\n","            <td>Organic</td>\n","            <td>238</td>\n","            <td>17</td>\n","            <td>7.14</td>\n","            <td>1846</td>\n","            <td>1571</td>\n","        </tr>\n","        <tr>\n","            <td>2019-05</td>\n","            <td>Facebook</td>\n","            <td>247</td>\n","            <td>14</td>\n","            <td>5.67</td>\n","            <td>1199</td>\n","            <td>997</td>\n","        </tr>\n","        <tr>\n","            <td>2019-05</td>\n","            <td>Instagram</td>\n","            <td>234</td>\n","            <td>11</td>\n","            <td>4.70</td>\n","            <td>959</td>\n","            <td>770</td>\n","        </tr>\n","        <tr>\n","            <td>2019-07</td>\n","            <td>Facebook</td>\n","            <td>558</td>\n","            <td>32</td>\n","            <td>5.73</td>\n","            <td>2222</td>\n","            <td>2144</td>\n","        </tr>\n","        <tr>\n","            <td>2019-08</td>\n","            <td>Facebook</td>\n","            <td>611</td>\n","            <td>18</td>\n","            <td>2.95</td>\n","           